/* components/fixedBottom/fixedBottom.wxss */
.fixed-bottom-wrap{
    display:flex;
    box-sizing: border-box;
    position: fixed;
    bottom:0;
    left:0;
    width:100%;
    padding:20rpx 0rpx;
    box-shadow: 0px -20rpx 10rpx rgba(255,255,255,0.9);
    background:#F6F6F6;
    z-index:1000;
}
.fixed-bottom-wrap button{
    flex:1;
    margin:0 20rpx;
    box-sizing: border-box;
}